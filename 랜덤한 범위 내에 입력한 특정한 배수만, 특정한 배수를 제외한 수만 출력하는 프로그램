#include <iostream>
using namespace std;

int main()
{
	srand(time(NULL));
	int num, count;    //num은 랜덤 숫자, count는 프로그램을 반복하는 횟수
	num = rand() % 100 + 1;    //1~100까지의 랜덤 숫자를 생성해서 변수 num에 저장
	cout << "랜덤한 수:" << num << endl;    //컴퓨터가 랜덤 숫자를 하나 출력
	cout << "특정 배수 개수:";
	cin >> count;    //프로그램을 몇 번 반복할 것인지, 그 횟수를 사용자가 입력
	cout << "\n";

	int i, n, k, m;    //i는 프로그램을 반복하는 횟수, k와 m은 1~랜덤숫자까지 반복하는 횟수, n은 특정 배수를 입력받는 변수
	for (i = 0; i < count; i++)    //입력받은 수(count)만큼 프로그램 반복 (for문 아래 문장들을 반복함)
	{
		cout << i + 1 << "번째 특정 배수:";
		cin >> n;    //특정 배수 입력
		cout << "<특정 배수만 출력하는 프로그램>" << endl;
		for (k = 1; k <= num; k++)    //1~랜덤숫자까지 반복
		{
			if (k % n == 0)    //1~랜덤숫자까지의 수들(k) 중 n으로 나누었을 때 나머지가 0인 값들 출력
				cout << k << ",";
		}
		cout << "\n";
		cout << "<특정 배수 제외 출력하는 프로그램>" << endl;
		for (m = 1; m <= num; m++)    //1~랜덤숫자까지 반복
		{
			if (m % n == 0)    //1~랜덤숫자까지의 수들(m) 중 n으로 나누었을 때 나머지가 0인 값들을 제외한 숫자들을 출력함
				continue;    //continue에 의해, m을 n으로 나누었을 때 나머지가 0인 값들은 제외됨
			cout << m << ",";
		}
		cout << "\n";
	}


	return 0;
}
